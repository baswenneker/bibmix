            <deduplication>
                <deduplication-condition acceptance-level="100">
                    <condition
                        class="cdc.impl.distance.EqualFieldsDistance"
                        column="id" weight="100">
                        <params/>
                    </condition>
                </deduplication-condition>
                <hashing-function columns="id,id" hash="equality"/>
            </deduplication>
        </preprocessing>
