    <join class="cdc.impl.join.snm.SNMJoin_v1" summary-left="false" summary-right="false">
        <params>
            <param name="window" value="8"/>
            <param name="sort-order-right" value="title"/>
            <param name="sort-order-left" value="title"/>
        </params>
        <join-condition class="cdc.impl.conditions.WeightedJoinCondition">
            <params>
                <param name="acceptance-level" value="100"/>
            </params>
            <condition class="cdc.impl.distance.PairSimilarityDistance"
                left-column="title" right-column="title" weight="100">
                <params>
                    <param name="disapprove-level" value="0.14"/>
                    <param name="approve-level" value="0.09"/>
                </params>
            </condition>
        </join-condition>
